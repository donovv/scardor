{
  "host": "localhost",
  "port": "PORT",
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "entity": "user",
    "service": "users",
    "secret": "SECRET",
    "authStrategies": [
      "jwt",
      "local"
    ],
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "https://Scardor.com",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "mongodb": "MONGODB",
  "ttv_client_id": "TTV_CLIENT_ID",
  "ttv_client_secret": "TTV_CLIENT_SECRET"
}
