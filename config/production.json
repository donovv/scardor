{
  "host": "scardor-backend-app.feathersjs.com",
  "port": "PORT",
  "authentication": {
    "entity": "user",
    "service": "users",
    "secret": "SECRET",
    "authStrategies": [
      "jwt",
      "local"
    ],
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "mongodb": "MONGODB"
}
